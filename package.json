{
	"name": "ssss",
	"version": "0.1.0",
	"private": true,
	"main": "src/main/index.js",
	"homepage": "./",
	"dependencies": {
		"@emotion/core": "^10.0.17",
		"@emotion/styled": "^10.0.17",
		"@reach/router": "^1.2.1",
		"@styled-system/css": "^5.0.23",
		"@styled-system/theme-get": "^5.0.21",
		"bip39": "^3.0.2",
		"cross-env": "^5.2.0",
		"drivelist": "^8.0.6",
		"electron-is-dev": "^1.1.0",
		"emotion-theming": "^10.0.17",
		"nodemon": "^1.19.1",
		"number-to-words": "^1.2.4",
		"pdfmake": "^0.1.58",
		"raw.macro": "^0.3.0",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-scripts": "3.1.1",
		"reflexbox": "^4.0.5",
		"slip39": "^0.1.5-dev.1",
		"styled-system": "^5.1.1",
		"svg-to-pdfkit": "^0.1.7"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"git add"
		],
		"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
			"prettier --single-quote --write",
			"git add"
		]
	},
	"scripts": {
		"react-start": "react-app-rewired start",
		"react-build": "react-app-rewired build",
		"react-eject": "react-app-rewired eject",
		"electron-build": "electron-builder",
		"release": "yarn react-build && electron-builder --publish=always",
		"build": "yarn react-build && yarn electron-build",
		"start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && NODE_ENV=dev nodemon --watch ./src/main/**/* --watch . --exec 'electron .'\"",
		"test": "react-app-rewired test",
		"coverage": "react-app-rewired test --coverage --watchAll=false",
		"postinstall": "electron-builder install-app-deps"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^4.1.0",
		"@testing-library/react": "^9.1.3",
		"concurrently": "^4.1.1",
		"electron": "^11.5.0",
		"electron-builder": "^21.1.5",
		"electron-devtools-installer": "^2.2.4",
		"husky": "^3.0.2",
		"jest-emotion": "^10.0.17",
		"jest-localstorage-mock": "^2.4.0",
		"lint-staged": "^9.2.1",
		"prettier": "^1.18.2",
		"react-app-rewired": "^2.1.3",
		"wait-on": "^3.3.0"
	},
	"jest": {
		"snapshotSerializers": [
			"jest-emotion"
		],
		"coverageReporters": [
			"text",
			"html"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	}
}
